<h4>Filter by Store Name</h4>
@for (int i = 0; i < ViewBag.store.Count; i++)
{
    <div>
        <ul>
            <li>
                <a href="#">@ViewBag.store[i]</a>
            </li>
        </ul>
    </div>
}

<script src="~/js/jquery-1.11.1.min.js" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $("li a").click(function () {
            var storename = $(this).text();
            debugger;
            debugger;
            $.ajax({
                type: "POST",
                url: '/Products/Productslist/',
                data: {
                    storename: storename,
                    id: 0,
                    page: 0
                },
                dataType: 'json'
            });
        });
    });
</script>
